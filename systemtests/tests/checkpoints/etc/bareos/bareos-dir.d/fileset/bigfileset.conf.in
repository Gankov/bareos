FileSet {
  Name = "bigfileset"
  Description = "fileset that contains the systemtest directory of the source directory"
  Include {
    Options {
      Signature = XXH128
      fstype = ext2
      fstype = ext3
      fstype = ext4
      fstype = overlay
      fstype = jfs
      fstype = ufs
      fstype = xfs
      fstype = zfs
      fstype = btrfs
    }
    File=<@tmpdir@/bigdata-list
  }
}
