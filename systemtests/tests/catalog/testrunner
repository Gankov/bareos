#!/bin/bash
set -e
set -o pipefail
set -u
#
TestName="$(basename "$(pwd)")"
export TestName

OS=$(uname)
if [[ $OS =~ MINGW64_.* ]];
then
  echo "Test $TestName does not work on windows"
  exit 77
fi

#shellcheck source=../environment.in
. ./environment

JobName=backup-bareos-fd
#shellcheck source=../scripts/functions
. "${rscripts}"/functions
"${rscripts}"/cleanup
"${rscripts}"/setup

exit_failure()
{
  estat=1
  end_test
}

start_test

${BAREOS_UNITTESTS_BINARY_DIR}/Debug/catalog || exit_failure

end_test
