#!/bin/bash
set -e
set -o pipefail
set -u
#
TestName="$(basename "$(pwd)")"
export TestName
#shellcheck source=../environment.in
. ./environment

JobName=backup-bareos-fd
#shellcheck source=../scripts/functions
. "${rscripts}"/functions

#TODO: get this work on windows
skip_if_windows "catalog backup does not work yet on windows"

"${rscripts}"/cleanup
"${rscripts}"/setup

exit_failure()
{
  estat=1
  end_test
}

start_test

pushd ${BAREOS_UNITTESTS_BINARY_DIR} >/dev/null
${BAREOS_UNITTESTS_BINARY_DIR}/${CMAKE_CONFIG_TYPE}/catalog || exit_failure

end_test
